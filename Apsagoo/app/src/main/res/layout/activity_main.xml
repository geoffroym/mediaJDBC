<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        tools:context=".ApsagooActivity" android:id="@+id/coordinatorLayout">
    <androidx.appcompat.widget.Toolbar
            android:layout_width="409dp"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_launcher_background"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize" android:id="@+id/toolbarBot"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/toolbarBg"/>
    <androidx.appcompat.widget.Toolbar
            android:layout_width="362dp"
            android:layout_height="445dp"
            android:background="?attr/colorPrimary"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize" android:id="@+id/toolbarBg"
            tools:layout_editor_absoluteY="150dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="23dp" android:layout_marginEnd="23dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <androidx.appcompat.widget.Toolbar
            android:layout_width="411dp"
            android:layout_height="115dp"
            android:background="@drawable/ic_launcher_background"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize" android:id="@+id/toolbarTop"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="28dp"
            app:layout_constraintBottom_toTopOf="@+id/toolbarBg" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <ImageView
            android:layout_width="73dp"
            android:layout_height="73dp" app:srcCompat="@drawable/pedrotesttherealalphaforreal"
            android:id="@+id/imageView3" tools:layout_editor_absoluteY="16dp" tools:layout_editor_absoluteX="16dp"
            android:rotation="17"/>
    <TextView
            android:text="@string/lorem_Ipsum"
            android:layout_width="356dp"
            android:layout_height="446dp" android:id="@+id/textView" tools:layout_editor_absoluteY="150dp"
            tools:layout_editor_absoluteX="26dp" android:textColor="#000000"/>
</androidx.constraintlayout.widget.ConstraintLayout>